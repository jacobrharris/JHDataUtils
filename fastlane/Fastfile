lane :build do |options|
	puts options
	
	ENV['GITHUB_API_TOKEN'] = options[:API_TOKEN]

	import_from_git(
	  url: 'https://github.com/AFNetworking/fastlane.git', 
	  branch: '0.0.2'
	)

	prepare_framework_release(
		skip_changelog:true
	)
	
	complete_framework_release(
		skip_ci_check:true
	)
end

